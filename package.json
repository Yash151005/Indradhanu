{
  "name": "indradhanu-aicrs",
  "version": "1.0.0",
  "description": "AI-Driven Global Climate Resilience System - Unified platform for Public Health, Marine Protection, and Circular Economy",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --prefix client",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "climate-resilience",
    "ai",
    "sustainability",
    "health",
    "marine",
    "circular-economy"
  ],
  "author": "Indradhanu Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
